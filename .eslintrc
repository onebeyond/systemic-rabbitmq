{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "rules": {
    "camelcase": 0,
    "no-param-reassign": "warn",
    "max-len": [
      "warn",
      120
    ],
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-constant-condition": 0,
    "no-console": 0,
    "no-tabs": 0,
    "consistent-return": "warn"
  }
}
